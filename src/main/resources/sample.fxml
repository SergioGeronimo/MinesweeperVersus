<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>


<GridPane
        prefHeight="500"
        prefWidth="500"
        gridLinesVisible="true"
        xmlns:fx="http://javafx.com/fxml/1"
        xmlns="http://javafx.com/javafx/11.0.1"
        fx:controller="sample.Controller">
    <columnConstraints>
        <ColumnConstraints/>
        <ColumnConstraints/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints />
        <RowConstraints />
    </rowConstraints>
    <GridPane
            gridLinesVisible="true"
            GridPane.columnIndex="0"
            GridPane.rowIndex="1"
            fx:id="playerGridPane">

        <columnConstraints>
            <ColumnConstraints prefWidth="50"/>
            <ColumnConstraints prefWidth="50"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints prefHeight="50"/>
            <RowConstraints prefHeight="50"/>
        </rowConstraints>

    </GridPane>

    <GridPane
            gridLinesVisible="true"
            GridPane.columnIndex="1"
            GridPane.rowIndex="1"
            fx:id="enemyGridPane">

    </GridPane>
   
</GridPane>
